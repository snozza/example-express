.comparison-container
  position relative
  float right
  min-width 100%
  max-width 100%
  height 254px
  margin-left auto
  margin-right auto
  padding-bottom 7px
  padding-left 4px
  //border 1px solid

  & .box
    box-shadow rgb(204, 204, 204) 1px 1px 10px 0px
    color rgb(80, 80, 80)
    height 254px
    width 9.2em
    float left
    border 1px solid rgb(255, 0, 51)
    border-radius 10px 10px 10px 10px
    font normal normal normal normal 16px/normal 'Open Sans', Helvetica, Arial, sans-serif

  & p
    color rgb(80, 80, 80)
    height 14px
    text-align left
    width 137px
    perspective-origin 71px 7px
    transform-origin 71px 7px
    border 0px none rgb(80, 80, 80)
    font normal normal 600 normal 10px/normal 'Open Sans', Helvetica, Arial, sans-serif
    margin 22px 0px 0px
    outline rgb(80, 80, 80) none 0px
    padding 0px 0px 0px 5px

  & input
    color rgb(80, 80, 80)
    height 20px
    width 128px
    border 1px solid rgb(211, 207, 208)
    font normal normal normal normal 12px/normal 'Open Sans', Helvetica, Arial, sans-serif
    margin 3px
    outline rgb(80, 80, 80) none 0px
    padding 3px
  
  & select
    height 14px
    width 202px
    background rgba(0, 0, 0, 0) none repeat scroll 0% 0% / auto padding-box border-box
    border 0px none rgb(80, 80, 80)
    font normal normal normal normal 13px/14px 'Open Sans', Helvetica, Arial, sans-serif
    padding 0px 0px 0px 6px

  & a
    color rgb(255, 255, 255)
    display block
    float left
    height 23px
    position relative
    text-align center
    text-decoration none
    width 71px
    perspective-origin 35.5px 11.5px
    transform-origin 35.5px 11.5px
    background rgb(51, 51, 51) none repeat scroll 0% 0% / auto padding-box border-box
    border 0px none rgb(255, 255, 255)
    font normal normal 100 normal 14px/22px 'Open Sans', Helvetica, Arial, sans-serif
    margin 0px 0px 0px 3px
    outline rgb(255, 255, 255) none 0px

  
